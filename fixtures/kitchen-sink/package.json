{
  "name": "kitchen-sink",
  "scripts": {
    "dev": "solid-start dev",
    "build": "solid-start build",
    "start": "solid-start start",
    "postinstall": "node ./fix-jest-dom.mjs",
    "test:unit": "npm-run-all test:unit:*",
    "test:unit:client": "TEST_ENV=client TEST_MODE=client vitest run",
    "test:unit:client-server": "TEST_ENV=client TEST_MODE=client-server vitest run",
    "test:unit:server": "TEST_ENV=server TEST_MODE=server vitest run",
    "start-server-and-test": "start-server-and-test build-and-start http://localhost:3000 test:e2e",
    "build-and-start": "npm run build && npm run start",
    "test": "npm run test:unit && npm run start-server-and-test",
    "test:e2e": "npm-run-all -p test:e2e:*",
    "test:e2e:js": "playwright test e2e/*.mjs",
    "test:e2e:no-js": "DISABLE_JAVASCRIPT=true playwright test e2e/*.mjs"
  },
  "type": "module",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.2",
    "@types/testing-library__jest-dom": "^5.14.3",
    "npm-run-all": "latest",
    "solid-app-router": "^0.3.0",
    "solid-js": "^1.3.13",
    "solid-meta": "^0.27.3",
    "solid-start": "workspace:*",
    "solid-start-node": "workspace:*",
    "solid-testing-library": "^0.3.0",
    "typescript": "^4.4.3",
    "undici": "^4.12.2",
    "vite": "^2.8.6",
    "vitest": "^0.6.1",
    "@magiql/ide": "^0.0.31",
    "cookie": "^0.4.1",
    "cookie-signature": "^1.1.0",
    "graphql": "^16.3.0",
    "graphql-helix": "^1.12.0",
    "playwright": "1.19.2",
    "@playwright/test": "^1.18.1",
    "start-server-and-test": "latest"
  },
  "engines": {
    "node": ">=14"
  }
}
